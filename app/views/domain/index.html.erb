<h1>Domainlist</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Url</th>
  </tr>
  
<% @domains.each do |domain|%>
  <tr>
  	<td><%= domain.name%></td>
  	<td><%= domain.url%></td>
  	<td> link_to 'Edit', edit_domain_path(domain)</td>
  </tr>
<%end%>
</table>

<%= link_to 'New Domain', new_domain_path %>